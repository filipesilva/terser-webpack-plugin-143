{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "one": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineStyle": true,
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/one",
      "sourceRoot": "projects/one/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/one",
            "index": "projects/one/src/index.html",
            "main": "projects/one/src/main.ts",
            "polyfills": "projects/one/src/polyfills.ts",
            "tsConfig": "projects/one/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/one/src/favicon.ico",
              "projects/one/src/assets"
            ],
            "styles": [
              "projects/one/src/assets/styles/styles.css"
            ],
            "scripts": [
              {
                "input": "projects/one/src/global-script.js"
              },
              {
                "input": "./node_modules/iban/iban.js"
              }
            ],
            "showCircularDependencies": false,
            "namedChunks": false,
            "i18nFormat": "xtb",
            "i18nMissingTranslation": "warning",
            "aot": true,
            "optimization": false,
            "buildOptimizer": false,
            "outputHashing": "all",
            "sourceMap": false,
            "statsJson": false,
            "extractCss": false,
            "extractLicenses": false,
            "vendorChunk": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/one/src/environments/environment.ts",
                  "with": "projects/one/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "assets": [],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "one:build",
            "disableHostCheck": true,
            "host": "0.0.0.0",
            "sourceMap": true
          },
          "configurations": {
            "production": {
              "browserTarget": "one:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "one:build"
          }
        }
      }
    }},
  "defaultProject": "one"
}
